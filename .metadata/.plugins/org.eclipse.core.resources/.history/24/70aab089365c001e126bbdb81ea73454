/* Definitions for LD1Task */
osThreadId_t LD1TaskHandle;
const osThreadAttr_t LD1Task_attributes = {
  .name = "LD1Task",
  .stack_size = 128 * 4,
  .priority = (osPriority_t) osPriorityNormal,
};
/* Definitions for LE2Task02 */
osThreadId_t LE2Task02Handle;
const osThreadAttr_t LE2Task02_attributes = {
  .name = "LE2Task02",
  .stack_size = 128 * 4,
  .priority = (osPriority_t) osPriorityLow,
};
/* Definitions for LD3Task03 */
osThreadId_t LD3Task03Handle;
const osThreadAttr_t LD3Task03_attributes = {
  .name = "LD3Task03",
  .stack_size = 128 * 4,
  .priority = (osPriority_t) osPriorityLow,
};

